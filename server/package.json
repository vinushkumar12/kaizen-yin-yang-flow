{
  "name": "kaizen-ml-backend",
  "version": "1.0.0",
  "description": "Machine Learning backend for Kaizen journaling app using Hugging Face models",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@huggingface/transformers": "^3.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "machine-learning",
    "hugging-face",
    "sentiment-analysis",
    "emotion-detection",
    "journaling",
    "mental-health",
    "ai"
  ],
  "author": "Kaizen Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
